    "prod:client": "webpack -p --progress --config=config/webpack.config.prod.js",
    "prod:server": "copyfiles -a -u 1 script/**/**/* build",
    "prebuild": "rimraf ../static/build",
    "build": "cross-env NODE_ENV=production webpack --config config/webpack.config.prod.js --progress",
